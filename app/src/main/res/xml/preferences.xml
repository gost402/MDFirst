<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <SwitchPreference
        android:defaultValue="false"
        android:key="@string/launcher_theme_dark_key"
        android:title="@string/preference_dark_theme" />

    <SwitchPreference
        android:defaultValue="false"
        android:key="@string/launcher_layout_dense_key"
        android:title="@string/preference_dense_layout" />

    <ListPreference
        android:defaultValue="1"
        android:entries="@array/preference_sorting_type_titles"
        android:entryValues="@array/preference_sorting_type"
        android:key="@string/launcher_sorting_type_key"
        android:negativeButtonText="@null"
        android:positiveButtonText="@null"
        android:title="@string/preference_sort_type" />

    <SwitchPreference
        android:defaultValue="true"
        android:key="@string/launcher_not_initialized_key"
        android:title="@string/preference_show_welcome_on_next_start" />
</PreferenceScreen>
